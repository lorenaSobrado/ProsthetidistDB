<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!DOCTYPE Company [
   <!ELEMENT Company (name, email, phone, Prosthetics)>
   <!ELEMENT name (#PCDATA)>
   <!ELEMENT email (#PCDATA)>
   <!ELEMENT phone (#PCDATA)>
   <!ELEMENT Prosthetics (price, functionalities, type, model, measurement, Materials)>
   <!ATTLIST Prosthetics code CDATA #REQUIRED>
   <!ELEMENT price (#PCDATA)>
   <!ELEMENT functionalities (#PCDATA)>
   <!ELEMENT type (#PCDATA)>
   <!ELEMENT model (#PCDATA)>
   <!ELEMENT measurement (#PCDATA)>
   <!ATTLIST measurement length CDATA #REQUIRED
   						 width CDATA  #REQUIRED
   						 weight CDATA >
   <!ELEMENT Materials (#PCDATA)
   <!ATTLIST Materials 	 name CDATA #REQUIRED
   						 price CDATA  #REQUIRED
   						 strength CDATA 
   						 flexibility CDATA
   						 temperatureResistance CDATA>
]>


